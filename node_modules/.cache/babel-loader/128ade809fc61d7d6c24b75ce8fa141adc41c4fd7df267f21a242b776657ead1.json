{"ast":null,"code":"export default {\n  name: 'Login',\n  metaInfo: {\n    title: 'Login - Quantum VMS',\n    meta: [{\n      property: 'og:title',\n      content: 'Login - Quantum VMS'\n    }]\n  },\n  data: function data() {\n    return {\n      // name:value pairs,\n      // name:value pairs\n      displayLogin: \"block\",\n      displayForgotPass: \"none\",\n      displaySignup: \"none\",\n      displayUnverified: \"none\"\n    };\n  },\n  methods: {\n    loginPage: function loginPage() {\n      this.displayLogin = \"block\", this.displayForgotPass = \"none\", this.displaySignup = \"none\", this.displayUnverified = \"none\", document.title = \"Login | calendaREADY\";\n    },\n    forgotPassPage: function forgotPassPage() {\n      this.displayLogin = \"none\", this.displayForgotPass = \"block\", this.displaySignup = \"none\", this.displayUnverified = \"none\", document.title = \"Forgot Password | calendaREADY\";\n    },\n    signupPage: function signupPage() {\n      this.displayLogin = \"none\", this.displayForgotPass = \"none\", this.displaySignup = \"block\", this.displayUnverified = \"none\", document.title = \"Sign Up | calendaREADY\";\n    },\n    unverifiedPage: function unverifiedPage() {\n      this.displayLogin = \"none\", this.displayForgotPass = \"none\", this.displaySignup = \"none\", this.displayUnverified = \"block\", document.title = \"Unverified Email | calendaREADY\";\n    }\n  }\n};","map":{"version":3,"names":["name","metaInfo","title","meta","property","content","data","displayLogin","displayForgotPass","displaySignup","displayUnverified","methods","loginPage","document","forgotPassPage","signupPage","unverifiedPage"],"sources":["src/views/login.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css\" rel=\"stylesheet\">\r\n        <link type=\"text/css\" rel=\"stylesheet\" href=\"https://unpkg.com/bootstrap/dist/css/bootstrap.min.css\" />\r\n        <link type=\"text/css\" rel=\"stylesheet\" href=\"https://unpkg.com/bootstrap-vue@latest/dist/bootstrap-vue.min.css\" />\r\n\r\n        <div class=\"page text-center\">\r\n\r\n            <!-- Login Box -->\r\n            <div class=\"main form-login w-100 m-auto\">\r\n\r\n                <!-- Logo -->\r\n                <img\r\n                    class=\"mb-1\"\r\n                    src=\"img/logo/logo-green.png\"\r\n                    alt=\"calendaREADY Logo\"\r\n                    width=\"128px\"\r\n                    height=\"128px\"\r\n                />\r\n\r\n                <div id=\"vue-app\">\r\n\r\n                    <!-- Login Component -->\r\n                    <div id=\"loginContent\" v-bind:style=\"{display: displayLogin}\">\r\n                        <form onsubmit=\"loginFunction(event)\">\r\n                            <!-- Login Header Text -->\r\n                            <h3>Welcome Back!</h3>\r\n                            <h4 class=\"mb-3 fw-normal\">Are you ready for a new day?</h4>\r\n\r\n                            <!-- Login with Google -->\r\n                            <button class=\"w-100 py-2 btn btn-outline-dark btn-light\" type=\"button\" onclick=\"googleLoginFunction(event)\">\r\n                                <img src=\"https://cdn-icons-png.flaticon.com/512/281/281764.png\" alt=\"Google\" height=\"16\" class=\"me-1\">\r\n                                Login with Google\r\n                            </button>\r\n\r\n                            <!-- Login Separator -->\r\n                            <p class=\"text-muted my-4 sep\"><span>or</span></p>\r\n\r\n                            <!-- Email Input Box -->\r\n                            <div class=\"form-floating mb-1\">\r\n                                <input type=\"email\" class=\"form-control\" id=\"loginEmail\" placeholder=\"test@test.com\" />\r\n                                <label for=\"loginEmail\">Email Address</label>\r\n                            </div>\r\n\r\n                            <!-- Password Input Box -->\r\n                            <div class=\"form-floating mb-1\">\r\n                                <input type=\"password\" class=\"form-control\" id=\"loginPassword\" placeholder=\"pw\" />\r\n                                <label for=\"loginPassword\">Password</label>\r\n                                <button class=\"passwordToggle\" type=\"button\" onclick=\"togglePassword(this)\"></button>\r\n                                <div id=\"forgotPassword\" class=\"form-text fst-italic text-start text-decoration-none text-primary\" v-on:click=\"forgotPassPage()\">Forgot your password? Click me!</div>\r\n                            </div>\r\n\r\n                            <!-- Login Errors component -->\r\n                            <div class=\"mb-3\" id=\"loginErrors\">\r\n                            </div>\r\n\r\n                            <!-- Login Button -->\r\n                            <button class=\"w-100 btn btn-lg button-login\" type=\"submit\" onclick=\"loginFunction(event)\">\r\n                                Login\r\n                            </button>\r\n\r\n                            <!-- Sign Up -->\r\n                            <p class=\"my-2\">Don't have an account? <span class=\"fw-bold text-decoration-none text-primary\" v-on:click=\"signupPage()\">Sign Up!</span></p>\r\n                        </form>\r\n                    </div>\r\n\r\n                    <!-- Forgot Password Component -->\r\n                    <div id=\"forgotPassContent\" v-bind:style=\"{display: displayForgotPass}\">\r\n                        <form onsubmit=\"forgotPassFunction(event)\">\r\n                            <!-- Forgot Password Header Text -->\r\n                            <h4>Forgot your Password?</h4>\r\n                            <p class=\"mb-3\">Enter your email address, and we'll send you a link to get back into your account.</p>\r\n\r\n                            <!-- Email Input Box -->\r\n                            <div class=\"form-floating mb-3\">\r\n                                <input type=\"email\" class=\"form-control\" id=\"forgotPassEmail\" placeholder=\"test@test.com\" />\r\n                                <label for=\"forgotPassEmail\">Email Address</label>\r\n                            </div>\r\n\r\n                            <!-- Forgot Password Errors component -->\r\n                            <div class=\"mb-3\" id=\"forgotPassErrors\">\r\n                            </div>\r\n\r\n                            <!-- Login Button -->\r\n                            <button class=\"w-100 btn btn-lg button-login\" type=\"submit\" onclick=\"forgotPassFunction(event)\" id=\"forgotPassButton\">\r\n                                Send Password Reset Link\r\n                            </button>\r\n\r\n                            <!-- Login Separator -->\r\n                            <p class=\"text-muted my-4 sep\"><span>or</span></p>\r\n\r\n                            <!-- Return to Login -->\r\n                            <h6 class=\"my-2 fw-bold text-decoration-none text-primary\" v-on:click=\"loginPage()\">Return to Login</h6>\r\n\r\n                            <!-- Sign Up -->\r\n                            <p class=\"my-2\">Don't have an account? <span class=\"fw-bold text-decoration-none text-primary\" v-on:click=\"signupPage()\">Sign Up!</span></p>\r\n                        </form>\r\n                    </div>\r\n\r\n                    <!-- Signup Component -->\r\n                    <div id=\"signupContent\" v-bind:style=\"{display: displaySignup}\">\r\n                        <form onsubmit=\"signupFunction(event)\">\r\n                            <!-- Signup Header Text -->\r\n                            <h3>Let's Get Started!</h3>\r\n                            <h4 class=\"mb-3 fw-normal\">A new day awaits you.<br>Create your account:</h4>\r\n\r\n                            <!-- Email Input Box -->\r\n                            <div class=\"form-floating mb-1\">\r\n                                <input type=\"email\" class=\"form-control\" id=\"signupEmail\" placeholder=\"test@test.com\" />\r\n                                <label for=\"signupEmail\">Email Address</label>\r\n                                <div id=\"signupEmailVerify\" class=\"form-text fst-italic text-start\">We'll send you a link to verify your email.</div>\r\n                            </div>\r\n\r\n                            <!-- Password Input Box -->\r\n                            <div class=\"form-floating mb-1\">\r\n                                <input type=\"password\" class=\"form-control\" id=\"signupPassword\" placeholder=\"pws\" />\r\n                                <label for=\"signupPassword\">Password</label>\r\n                                <button class=\"passwordToggle\" type=\"button\" onclick=\"toggleSignupPassword(this)\"></button>\r\n                            </div>\r\n\r\n                            <!-- Confirm Password Input Box -->\r\n                            <div class=\"form-floating mb-2\">\r\n                                <input type=\"password\" class=\"form-control\" id=\"signupPasswordConfirm\" placeholder=\"pwc\" />\r\n                                <label for=\"signupPasswordConfirm\">Confirm Password</label>\r\n                                <button class=\"passwordToggle\" type=\"button\" onclick=\"toggleSignupConfirmPassword(this)\"></button>\r\n                            </div>\r\n\r\n                            <p class=\"text-muted fst-italic mb-3\">Tip: You can follow clubs in the Profile page!</p>\r\n\r\n                            <!-- Signup Errors component -->\r\n                            <div class=\"mb-3\" id=\"signupErrors\">\r\n                            </div>\r\n\r\n                            <!-- Signup Button -->\r\n                            <button class=\"w-100 btn btn-lg button-login\" type=\"submit\" onclick=\"signupFunction(event)\">\r\n                                Sign Up\r\n                            </button>\r\n                            <p class=\"text-muted mb-0 fw-light\">By signing up, you agree to our Terms of Use.</p>\r\n\r\n                            <!-- Login -->\r\n                            <p class=\"my-2\">Already have an account? <span class=\"fw-bold text-decoration-none text-primary\" v-on:click=\"loginPage()\">Login!</span></p>\r\n                        </form>\r\n                    </div>\r\n\r\n                    <!-- Unverified Component -->\r\n                    <div id=\"unverifiedContent\" v-bind:style=\"{display: displayUnverified}\">\r\n                        <!-- Unverified Header Text -->\r\n                        <h4>Wait a moment!</h4>\r\n                        <p class=\"fw-bold mb-1\">To help you take control of your day, we first need you to verify your email.</p>\r\n                        <p class=\"mb-1\">Please check your inbox for our email!</p>\r\n                        <p class=\"mb-3 fst-italic\">It may be in your spam inbox.</p>\r\n\r\n                        <!-- Login Separator -->\r\n                        <p class=\"text-muted my-4 sep\"><span>or</span></p>\r\n\r\n                        <!-- Resend Verify Errors component -->\r\n                        <div class=\"mb-3\" id=\"resendVerifyErrors\">\r\n                        </div>\r\n\r\n                        <!-- Resend Verification Email Button -->\r\n                        <button class=\"w-100 btn btn-lg button-login\" type=\"button\" onclick=\"sendVerificationEmailFunction(event)\" id=\"resendVerifyButton\">\r\n                            Resend Verification Link\r\n                        </button>\r\n\r\n                        <!-- Return to Login -->\r\n                        <h6 class=\"my-2 fw-bold text-decoration-none text-primary\" v-on:click=\"loginPage()\" onclick=\"logoutFunction()\">Return to Login</h6>\r\n\r\n                        <!-- Refresh Page Message -->\r\n                        <p class=\"my-2\">If you've just verified your email,<br>please refresh this page to continue!</p>\r\n                    </div>\r\n\r\n                </div>\r\n\r\n            </div>\r\n\r\n            <!-- Background Image Carousel -->\r\n            <div id=\"loginBackgroundCarousel\" class=\"carousel slide carousel-fade\" data-bs-ride=\"carousel\">\r\n                <div class=\"carousel-inner\">\r\n                    <div class=\"carousel-item active\" data-bs-interval=\"15000\">\r\n                        <img src=\"https://static.wixstatic.com/media/4ebc73_d58c86562c7249aeba90bdec58011d57~mv2.jpg/v1/fill/w_980,h_653,al_c,q_85,usm_0.66_1.00_0.01,enc_auto/4ebc73_d58c86562c7249aeba90bdec58011d57~mv2.jpg\" alt=\"\" class=\"vh-100 min-vw-100\">\r\n                    </div>\r\n                    <div class=\"carousel-item\" data-bs-interval=\"15000\">\r\n                        <img src=\"https://static.wixstatic.com/media/4ebc73_784090313a5a4c238c97b159e0dd3e23~mv2.jpg/v1/fill/w_980,h_654,al_c,q_85,usm_0.66_1.00_0.01,enc_auto/4ebc73_784090313a5a4c238c97b159e0dd3e23~mv2.jpg\" alt=\"\" class=\"vh-100 min-vw-100\">\r\n                    </div>\r\n                    <div class=\"carousel-item\" data-bs-interval=\"15000\">\r\n                        <img src=\"https://static.wixstatic.com/media/4ebc73_cbd86a51ce254587a21dbc5b925ca1e0~mv2.jpg/v1/fill/w_980,h_653,al_c,q_85,usm_0.66_1.00_0.01,enc_auto/4ebc73_cbd86a51ce254587a21dbc5b925ca1e0~mv2.jpg\" alt=\"\" class=\"vh-100 min-vw-100\">\r\n                    </div>\r\n                    <div class=\"carousel-item\" data-bs-interval=\"15000\">\r\n                        <img src=\"https://static.wixstatic.com/media/4ebc73_ea40b5ff373f42718e4513e7ca58a6a1~mv2.jpg/v1/fill/w_980,h_586,al_c,q_85,usm_0.66_1.00_0.01,enc_auto/4ebc73_ea40b5ff373f42718e4513e7ca58a6a1~mv2.jpg\" alt=\"\" class=\"vh-100 min-vw-100\">\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: 'Login',\r\n        metaInfo: {\r\n            title: 'Login - Quantum VMS',\r\n            meta: [\r\n                {\r\n                    property: 'og:title',\r\n                    content: 'Login - Quantum VMS',\r\n                },\r\n            ],\r\n        },\r\n        data() {\r\n            return {\r\n                // name:value pairs,\r\n                // name:value pairs\r\n                displayLogin: \"block\",\r\n                displayForgotPass: \"none\",\r\n                displaySignup: \"none\",\r\n                displayUnverified: \"none\"\r\n            }\r\n        },\r\n        methods: {\r\n            loginPage() {\r\n                this.displayLogin = \"block\",\r\n                this.displayForgotPass = \"none\",\r\n                this.displaySignup = \"none\",\r\n                this.displayUnverified = \"none\",\r\n                document.title = \"Login | calendaREADY\"\r\n            },\r\n            forgotPassPage() {\r\n                this.displayLogin = \"none\",\r\n                this.displayForgotPass = \"block\",\r\n                this.displaySignup = \"none\",\r\n                this.displayUnverified = \"none\",\r\n                document.title = \"Forgot Password | calendaREADY\"\r\n            },\r\n            signupPage() {\r\n                this.displayLogin = \"none\",\r\n                this.displayForgotPass = \"none\",\r\n                this.displaySignup = \"block\",\r\n                this.displayUnverified = \"none\",\r\n                document.title = \"Sign Up | calendaREADY\"\r\n            },\r\n            unverifiedPage() {\r\n                this.displayLogin = \"none\",\r\n                this.displayForgotPass = \"none\",\r\n                this.displaySignup = \"none\",\r\n                this.displayUnverified = \"block\",\r\n                document.title = \"Unverified Email | calendaREADY\"\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    .page {\r\n        height: 100%;\r\n    }\r\n    .main {\r\n        height: 100%;\r\n        display: flex;\r\n        align-items: center;\r\n        padding-top: 40px;\r\n        padding-bottom: 40px;\r\n        background-color: #2a2b2e;\r\n    }\r\n\r\n    .form-login {\r\n        max-width: 350px;\r\n        padding: 15px;\r\n        border: 5px solid #4f646f;\r\n        border-radius: 2%;\r\n        background-color: #eeeeee;\r\n    }\r\n\r\n    .sep {\r\n        border-bottom: 1px solid #aaaaaa;\r\n        line-height: 0.1rem;\r\n    }\r\n\r\n    .sep span {\r\n        background: #eeeeee;\r\n        padding: 0 10px;\r\n    }\r\n\r\n    .button-login {\r\n        background-color: #104547;\r\n        color: white;\r\n    }\r\n\r\n    .button-login:hover {\r\n        background-color: #3F6A6B;\r\n        color: white;\r\n    }\r\n\r\n    .carousel {\r\n        z-index: -99;\r\n        position: fixed;\r\n        width: 100%;\r\n        height: 100%;\r\n        opacity: 40%;\r\n    }\r\n\r\n    .passwordToggle {\r\n        width: 24px;\r\n        height: 24px;\r\n        border: 0;\r\n        background: transparent;\r\n        background-image: url(\"https://cdn-icons-png.flaticon.com/512/2767/2767194.png\");\r\n        opacity: 40%;\r\n        background-size: 100%;\r\n        background-position: center;\r\n        background-repeat: no-repeat;\r\n        position: absolute;\r\n        right: 16px;\r\n        translate: 0 -165%;\r\n    }\r\n    .passwordToggle.showing {\r\n        background-image: url(\"https://cdn-icons-png.flaticon.com/512/2767/2767146.png\");\r\n    }\r\n</style>"],"mappings":"AAsMA;EACAA,IAAA;EACAC,QAAA;IACAC,KAAA;IACAC,IAAA,GACA;MACAC,QAAA;MACAC,OAAA;IACA;EAEA;EACAC,IAAA,WAAAA,KAAA;IACA;MACA;MACA;MACAC,YAAA;MACAC,iBAAA;MACAC,aAAA;MACAC,iBAAA;IACA;EACA;EACAC,OAAA;IACAC,SAAA,WAAAA,UAAA;MACA,KAAAL,YAAA,YACA,KAAAC,iBAAA,WACA,KAAAC,a